#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'flyt-python==1.1.2','console_scripts','flyt_python'
__requires__ = 'flyt-python==1.1.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('flyt-python==1.1.2', 'console_scripts', 'flyt_python')()
    )
